package com.techlabs.iterative.test;

public class ArmstrongNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int number = 153;
		
		int sum=0;
		
		int cube =0;
		
		int digit=0;
		
		while(number>0) {
			digit=digit %10;
			cube = number*number*number;
			sum = sum +cube;
			number = number/10;
		}
	}

}
